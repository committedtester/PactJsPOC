{
  "consumer": {
    "name": "WebApp"
  },
  "provider": {
    "name": "LocalTokenAPI"
  },
  "interactions": [
    {
      "description": "Get user token with token ID",
      "providerState": "The server has User Tokens",
      "request": {
        "method": "GET",
        "path": "/token/1234",
        "headers": {
          "Accept": "application/json, text/plain, */*"
        }
      },
      "response": {
        "status": 202,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "token": "bearer"
        },
        "matchingRules": {
          "$.body.token": {
            "match": "type"
          }
        }
      }
    },
    {
      "description": "Get user token with NULL token ID",
      "providerState": "The server has User Tokens",
      "request": {
        "method": "GET",
        "path": "/token/",
        "headers": {
          "Accept": "application/json, text/plain, */*"
        }
      },
      "response": {
        "status": 400,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
        }
      }
    },
    {
      "description": "Get user token with invalid String token ID",
      "providerState": "The server has User Tokens",
      "request": {
        "method": "GET",
        "path": "/token/invalidString",
        "headers": {
          "Accept": "application/json, text/plain, */*"
        }
      },
      "response": {
        "status": 500,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}